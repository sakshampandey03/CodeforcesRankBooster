{
  "manifest_version": 3,
  "name": "Codeforces Popular Problems",
  "version": "1.0",
  "description": "Extracts top 5 popular problems by given difficulty rating from Codeforces.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
